<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import com.jfoenix.controls.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" prefHeight="333.0" prefWidth="394.0" spacing="10.0" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.view.speciality.ViewSpecialityController">
   <children>
      <TableView fx:id="specTable" VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn fx:id="specialityCol" prefWidth="75.0" text="Speciality" />
            <TableColumn fx:id="descriptionCol" prefWidth="75.0" text="Description" />
         </columns>
         <effect>
            <DropShadow />
         </effect>
      </TableView>
      <HBox alignment="CENTER_LEFT" spacing="10.0" style="-fx-background-color: white;" styleClass="VBox" VBox.vgrow="ALWAYS">
         <children>
            <JFXButton buttonType="RAISED" onAction="#search" style="-fx-background-color: #1976D2;" text="Search" textFill="WHITE">
               <font>
                  <Font name="Roboto Bold" size="16.0" />
               </font>
            </JFXButton>
            <JFXTextField fx:id="search_info" maxWidth="1.7976931348623157E308" minWidth="205.0" promptText="Input speciality" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="Roboto" size="16.0" />
               </font>
            </JFXTextField>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" top="5.0" />
         </padding>
         <effect>
            <DropShadow />
         </effect>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
